<p id="notice"><%= notice %></p>

<h1>Listing Attendance Sheets</h1>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendance_sheets.each do |attendance_sheet| %>
      <tr>
        <td><%= attendance_sheet.course %></td>
        <td><%= link_to 'Show', attendance_sheet %></td>
        <td><%= link_to 'Edit', edit_attendance_sheet_path(attendance_sheet) %></td>
        <td><%= link_to 'Destroy', attendance_sheet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attendance sheet', new_attendance_sheet_path %>
